<Table>:

    orientation: 'vertical'

    canvas.before:
        Color:
            rgba: 0.13, 0.14, 0.15, 1
        Rectangle:
            # self here refers to the widget i.e FloatLayout
            pos: self.pos
            size: self.size

<Cell>:

    selected_color: 0.58, 0.58, 0.58, 1
    deselected_color: 0.07, 0.074, 0.078, 0.9
    markup: True
